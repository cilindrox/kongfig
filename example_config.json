{
  "host": "localhost:8001",
  "https": false,
  "version": "0.13.1",
  "services": [
    {
      "name": "api-providers",
      "url": "https://api-providers.pagerinc.in",
      "retries": 5,
      "connect_timeout": 800,
      "read_timeout": 1000,
      "write_timeout": 1000,
      "routes": [
        {
          "paths": [
            "/login",
            "/forgot",
            "/reset"
          ],
          "protocols": ["https"],
          "methods": ["POST"],
          "hosts": ["api-providers.pagerinc.in"],
          "strip_path": false
        }
      ]
    }
  ]
}
